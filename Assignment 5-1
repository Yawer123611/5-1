
a = 1
b = 2
c = 5 
while a < c:
    b = b + c 
print(a,b,c)

d = 4
e = 6
f = 7
while d > f:
    d = d + 1 
    e = e - 1 
print (d,e,f)

g = 4
h = 6
while g < h:
    g = g + 1 
print (g,h)

j = 2
k = 5 
n = 9 
while j > k:
    m = 6 
    while m < n:
        print("Goodbye")
j = j + 1

j = 2
k = 5
m = 6
n = 9 
while j < k:
    while m < n:
        print("Hello")
j = j + 1 

p = 2 
q = 4 
while p < q:
    print("Adios")
    r = 1
p = p + 1